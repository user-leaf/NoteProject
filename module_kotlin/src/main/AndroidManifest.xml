<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sesame.module_kotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".KotlinApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NoteProject"
        android:usesCleartextTraffic="true">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".coroutine.CoroutineActivity" />
        <activity android:name=".learning.KotlinStandardFunctionActivity" />
        <activity android:name=".jetpack.lifecycle.LifecycleActivity" />
        <activity android:name=".jetpack.viewmodel.VmDemoActivity" />
        <activity android:name="com.sesame.module_kotlin.jetpack.viewmodel.sharedata.SeekBarActivity" />
        <activity android:name=".jetpack.databinding.DataBindingDemoActivity" />

        <service android:name=".jetpack.lifecycle.service.MyService" />
    </application>

</manifest>